module test;
import io;

func pmemcpy(dest: i8*, src: i8*, size: usz) -> void {
    for (let i: usz = 0; i < size; i += 1) {
        dest[i] = src[i];
    }
}

func memcpy(dest: void[], src: void[]) -> void {
    pmemcpy(&dest[0], &src[0], len(dest));
}

